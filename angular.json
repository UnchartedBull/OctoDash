{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "e1569209-2b82-4195-9c15-0669a78feccb"
  },
  "newProjectRoot": "projects",
  "projects": {
    "OctoDash": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "9mb",
                  "maximumWarning": "8mb",
                  "type": "initial"
                },
                {
                  "maximumWarning": "8kb",
                  "type": "anyComponentStyle"
                }
              ],
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "localize": true,
              "namedChunks": false,
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "allowedCommonJsDependencies": [
              "lottie-web"
            ],
            "aot": true,
            "assets": [
              {
                "glob": "!(config.testing.json)",
                "input": "src/assets/",
                "output": "/assets/"
              },
              {
                "glob": "*",
                "input": "src/assets/icon/",
                "output": "/assets/icon/"
              },
              {
                "glob": "*",
                "input": "src/assets/animations/",
                "output": "/assets/animations/"
              },
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              }
            ],
            "browser": "src/main.ts",
            "i18nMissingTranslation": "warning",
            "index": "src/index.html",
            "localize": false,
            "outputPath": {
              "base": "dist",
              "browser": ""
            },
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "scripts": [],
            "styles": [
              "src/reset.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "OctoDash:serve:production"
            }
          },
          "options": {
            "devServerTarget": "OctoDash:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "OctoDash:build:production"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "tsconfig.json"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "OctoDash:build:development"
            },
            "production": {
              "buildTarget": "OctoDash:build:production"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "OctoDash:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "scripts": [],
            "styles": [
              "src/reset.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "i18n": {
        "locales": {
          "da": {
            "baseHref": "",
            "translation": "src/locale/messages.da.xlf"
          },
          "de": {
            "baseHref": "",
            "translation": "src/locale/messages.de.xlf"
          },
          "fr": {
            "baseHref": "",
            "translation": "src/locale/messages.fr.xlf"
          }
        },
        "sourceLocale": {
          "baseHref": "",
          "code": "en"
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    },
    "@schematics/angular:service": {
      "type": "service"
    }
  },
  "version": 1
}
