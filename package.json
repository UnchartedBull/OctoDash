{
  "name": "octodash",
  "description": "OctoDash is a simple, but beautiful dashboard for Octoprint.",
  "version": "2.7.1",
  "license": "Apache 2.0",
  "homepage": "https://github.com/UnchartedBull/OctoDash",
  "author": {
    "email": "timon.gaebelein@icloud.com",
    "name": "Timon Gaebelein"
  },
  "browserslist": [
    "chrome >= 120"
  ],
  "build": {
    "appId": "octodash",
    "directories": {
      "output": "package"
    },
    "files": [
      "main.js",
      "src/helper",
      "dist",
      "angular.json",
      "node_modules",
      "themes/**/*.css",
      "LICENSE.md",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "linux": {
      "category": "Utility",
      "icon": "./src/assets/icon/icon.png",
      "target": [
        {
          "arch": [
            "armv7l",
            "arm64",
            "x64"
          ],
          "target": "deb"
        }
      ]
    },
    "productName": "OctoDash"
  },
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/localize": "^21.0.3",
    "@angular/material": "^21.0.2",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@fortawesome/angular-fontawesome": "^4.0.0",
    "@fortawesome/free-solid-svg-icons": "~7.2.0",
    "ajv": "~8.17.1",
    "angular-svg-round-progressbar": "^15.0.0",
    "bonjour-service": "^1.3.0",
    "compare-versions": "~6.1.1",
    "electron-store": "~10.1.0",
    "got": "^14.6.6",
    "lodash-es": "~4.17.23",
    "lottie-web": "~5.13.0",
    "mime-types": "^3.0.2",
    "ngx-lottie": "~21.2.0",
    "progress-stream": "~2.0.0",
    "rxjs": "~7.8.1",
    "tslib": "~2.8.1",
    "xliff": "~6.3.0",
    "zone.js": "~0.15.1"
  },
  "description": "OctoDash is a simple, but beautiful dashboard for Octoprint.",
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@angular/language-service": "^21.0.3",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/got": "~9.6.12",
    "@types/lodash-es": "~4.17.12",
    "@types/mime-types": "^3.0.1",
    "@types/node": "24.10.2",
    "codelyzer": "~6.0.2",
    "electron": "^39.2.7",
    "electron-builder": "~26.7.0",
    "eslint": "9.39.2",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-prettier": "~5.5.5",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "globals": "^17.3.0",
    "json-schema-to-typescript": "^15.0.4",
    "npm-run-all": "~4.1.5",
    "prettier": "^3.8.1",
    "stylelint": "^17.2.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "ts-node": "~10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "wait-on": "~9.0.3"
  }
}
